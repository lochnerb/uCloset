            <div id="dvBackground" style="z-index: 1004; display: table-cell; background-image: url(./Images/HeaderBackground.gif); background-color: #8e8787; height: 30px; width: 708px; display: table-cell; background-repeat: repeat; padding: 0px;">
                <img src="Images/Hangers.png" height="129" />
                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                <img style="z-index:1004;" src="Images/uCloset_Title_1140823.gif" />
            </div>
            <div id="dvRightHeaderIMG" style="display: table-column; background-image: url(./Images/grayFiller.jpg); background-color: #8e8787; height: 30px; width: 222px; display: table-cell; padding: 0px; background-repeat: repeat; ">
                <img src="Images/Mannequin_Stand.jpg" />
            </div>
            <div id="dvRightHeaderBar" style="display: table-column; background-image: url(./Images/RightHeaderBar.gif); background-color: #8e8787; height: 30px; width: 35px; display: table-cell; background-repeat: repeat; padding: 0px; "></div>
        </div>

        <div class="navbar navbar-inverse navbar-fixed-top" style="z-index:999;vertical-align:top;border-style:hidden;border-color:black;border-width:thin;width: 475px; position: absolute; top:95px; height: 10px; left: 265px; text-align: right; background-color: #8e8787; text-decoration-color: white;">
            <ul class="nav navbar-nav" style="z-index: 999; text-align: right; align-content: flex-end; border-style: hidden; align-items: flex-end; text-align-last: right; text-decoration-color: black; text-emphasis-color: black;">
                <!--<li style="z-index: 999;"><a id=" a1" href="~/"><asp:label id="lblHome" style="border:hidden;" forecolor="White">Home</asp:label></a></li>-->
                <li style="z-index: 999;">@Html.ActionLink("Home", "Index", "Landing")</li>
                <!-- <li style="z-index: 999;">@Html.ActionLink("About", "Index", "About")</li> -->
                <li style="z-index: 999;">@Html.ActionLink("Wardrobe", "Index", "Wardrobe")</li>
                <li style="z-index: 999;">@Html.ActionLink("Outfit", "Index", "Outfit")</li>
                <li style="z-index: 999;">@Html.ActionLink("F.A.Q.", "Index", "FAQ")</li>
                <li style="z-index: 999;">@Html.ActionLink("Merchant", "Index", "Merchant")</li>
